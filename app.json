{
  "expo": {
    "name": "app_radarrearthrn",
    "slug": "app_radarrearthrn",
    "scheme": "com.radarrearth.app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "bundleIdentifier": "com.radarrearth.app",
      "supportsTablet": true
    },
    "android": {
      "package": "com.radarrearth.app",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyDqD9gbgL0aV9_xluJog6ozteYK2OXJnos"
        }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "b37772db-4069-4b09-8897-e199c17ccc72"
      }
    },
    "owner": "jotaemepe",
    "plugins": [
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ],
      "expo-localization"
    ]
  }
}
